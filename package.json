{
  "name": "emulate",
  "displayName": "Emulate",
  "description": "Emulate Android and iOS easily!",
  "version": "0.0.1",
  "publisher": "diemasmichiels",
  "author": {
    "name": "Diemas Michiels"
  },
  "engines": {
    "vscode": "^1.22.0"
  },
  "categories": ["Debuggers", "Other"],
  "activationEvents": ["onCommand:extension.emulate"],
  "main": "./src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.emulate",
        "title": "Emulate"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "eslint": "^4.11.0",
    "vscode": "^1.1.6"
  },
  "dependencies": {
    "child_process": "^1.0.2"
  }
}
