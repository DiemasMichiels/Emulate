{
  "name": "emulate",
  "displayName": "Android iOS Emulator",
  "description": "Run Android emulator and iOS simulator easily from VScode!",
  "version": "0.0.4",
  "preview": true,
  "publisher": "DiemasMichiels",
  "author": {
    "name": "Diemas Michiels"
  },
  "engines": {
    "vscode": "^1.22.0"
  },
  "categories": [
    "Debuggers",
    "Other"
  ],
  "keywords": [
    "Emulator",
    "Android",
    "iOS",
    "Simulator"
  ],
  "activationEvents": [
    "onCommand:extension.emulator"
  ],
  "main": "./src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.emulator",
        "title": "Emulator"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "eslint": "^4.11.0",
    "vscode": "^1.1.6"
  },
  "dependencies": {
    "child_process": "^1.0.2"
  },
